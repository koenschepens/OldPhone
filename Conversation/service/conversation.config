[immediateActions]
stop=executeAction(Stop)
play=executeAction(Play)
mute=executeAction(Mute)
next=executeAction(SkipNext)
previous=executeAction(SkipPrevious)
left=executeAction(Left)
right=executeAction(Right)
down=executeAction(Down)
up=executeAction(Up)
back=executeAction(Back)
info=executeAction(Info)

[actions]
;weather.search=weather(location=$location,speech=$speech)
weather.search=show_notification(title=$location,message=$speech)
media.video_play=playvideo(service_name=$service_name,searchQuery=$q)
smalltalk.greetings=show_notification(title=$resolvedQuery,message=$speech)
wisdom.unknown=show_notification(title=$resolvedQuery,message=$speech)
